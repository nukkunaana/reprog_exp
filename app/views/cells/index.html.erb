<h1>Listing cells</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Passage</th>
      <th>Date</th>
      <th>Container</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @cells.each do |cell| %>
      <tr>
        <td><%= cell.cell_type %></td>
        <td><%= cell.passage %></td>
        <td><%= cell.date %></td>
        <td><%= cell.container %></td>
        <td><%= link_to 'Show', cell %></td>
        <td><%= link_to 'Edit', edit_cell_path(cell) %></td>
        <td><%= link_to 'Destroy', cell, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cell', new_cell_path %>
